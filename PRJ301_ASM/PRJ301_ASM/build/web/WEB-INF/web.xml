<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <context-param>
        <param-name>canvasW</param-name>
        <param-value>500</param-value>
    </context-param>
    <context-param>
        <param-name>canvasH</param-name>
        <param-value>500</param-value>
    </context-param>
    <servlet>
        <servlet-name>RectController</servlet-name>
        <servlet-class>dynamic.RectController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RectView</servlet-name>
        <servlet-class>view.RectView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestRectController</servlet-name>
        <servlet-class>dynamic.TestRectController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>dynamic.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListStudentController</servlet-name>
        <servlet-class>controller.student.ListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertController</servlet-name>
        <servlet-class>controller.student.InsertController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStudentController</servlet-name>
        <servlet-class>controller.student.UpdateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteStudentController</servlet-name>
        <servlet-class>controller.student.DeleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchByDidController</servlet-name>
        <servlet-class>controller.student.SearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.authentication.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>controller.authentication.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeTableController</servlet-name>
        <servlet-class>controller.lecturer.TimeTableController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeLecturerController</servlet-name>
        <servlet-class>controller.lecturer.HomeLecturerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AttendanceTakingController</servlet-name>
        <servlet-class>controller.lecturer.AttendanceTakingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkReportController</servlet-name>
        <servlet-class>controller.lecturer.MarkReportController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PresentReportController</servlet-name>
        <servlet-class>controller.lecturer.PresentReportController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentMarkReportController</servlet-name>
        <servlet-class>controller.student.MarkReportController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentPresentReportController</servlet-name>
        <servlet-class>controller.student.PresentReportController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentTimeTableController</servlet-name>
        <servlet-class>controller.student.TimeTableController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendMailWithStudentEmail</servlet-name>
        <servlet-class>SendMail.SendMailWithStudentEmail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RectController</servlet-name>
        <url-pattern>/rect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RectView</servlet-name>
        <url-pattern>/view/rect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestRectController</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListStudentController</servlet-name>
        <url-pattern>/student/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertController</servlet-name>
        <url-pattern>/student/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStudentController</servlet-name>
        <url-pattern>/student/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteStudentController</servlet-name>
        <url-pattern>/student/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchByDidController</servlet-name>
        <url-pattern>/student/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimeTableController</servlet-name>
        <url-pattern>/lecturer/timetable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeLecturerController</servlet-name>
        <url-pattern>/homelecturer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AttendanceTakingController</servlet-name>
        <url-pattern>/lecturer/att</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkReportController</servlet-name>
        <url-pattern>/lecturer/markreport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PresentReportController</servlet-name>
        <url-pattern>/lecturer/presentreport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentMarkReportController</servlet-name>
        <url-pattern>/student/markreport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentPresentReportController</servlet-name>
        <url-pattern>/student/presentreport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentTimeTableController</servlet-name>
        <url-pattern>/student/timetable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendMailWithStudentEmail</servlet-name>
        <url-pattern>/sendmail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>25</session-timeout>
    </session-config>
</web-app>
